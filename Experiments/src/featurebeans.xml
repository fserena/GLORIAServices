			<map>
				<entry key="rt">
					<bean
						class="eu.gloria.gs.services.experiment.base.models.ParameterFeature"
						scope="singleton">
						<property name="parameter" ref="OBJECT"></property>
						<property name="parameterArguments">
							<list>
								<value>
									{
									"name" : null,
									"order" : 0
									}
								</value>
							</list>
						</property>
					</bean>
				</entry>
			</map>
		</property>
		<property name="Operations">
			<map>
				<entry key="load_rt_name">
					<bean
						class="eu.gloria.gs.services.experiment.base.models.OperationFeature"
						scope="singleton">
						<property name="operation" ref="LoadRTName"></property>
						<property name="relations">
							<list>
								<value>rt.order</value>
								<value>rt.name</value>
							</list>
						</property>
					</bean>
				</entry>
			</map>
		</property>
	</bean>

